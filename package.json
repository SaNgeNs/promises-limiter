  "keywords": [
    "promises",
    "limiter",
    "async",
    "concurrency",
    "batching",
    "delay",
    "abort",
    "javascript",
    "typescript",
    "rate-limiting",
    "progress-callback",
    "promise-limiter",
    "async-limiter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SaNgeNs/promises-limiter"
  },
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run type-check && npm run test && tsup",
    "type-check": "npx tsc --noEmit",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "prepare": "husky"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3"
  }
}
